{
    "manifest_version": 2,
    "name": "ServiceNow Chrome Extension",
    "version": "0.0.2",
    "content_scripts": [
        {
            "matches": [
                "https://*.service-now.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs"
    ]
}